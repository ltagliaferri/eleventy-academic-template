<footer role="contentinfo">
  <div id="footer--container">
    <div id="footer--left">
      <a href="{{ site.githubUrl }}">
        {{ site.name }} 
      </a>
    </div>
    <div id="footer--middle">
        {% svgsprite %}
          <a href="{{ site.twitterUrl }}" title="twitter">{% icon "twitter" %}</a>
          <a href="{{ site.githubUrl }}/tree/master/{{page.inputPath}}" title="github">{% icon "github" %}</a>
          <a href="{{ site.linkedInUrl }}" title="linkedin">{% icon "linkedin" %}</a>
          <a href="{{ site.instagramURL }}" title="instagram">{% icon "instagram" %}</a>
    </div>
    <div id="footer--right"> 
      &copy; {{ site.copyright }}
    </div>
  </div>
</footer>